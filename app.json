{
  "expo": {
    "name": "askApiko",
    "description": "Apiko React Native course demo application",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "splash": {
        "image": "./assets/images/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "bundleIdentifier": "com.askApiko.askApiko",
      "supportsTablet": false
    },
    "android": {
      "splash": {
        "image": "./assets/images/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "package": "com.askApiko.askApiko"
    },
    "sdkVersion": "29.0.0"
  }
}
